<Window x:Class="Продвинутая_лабораторная_работа_по_симплекс_методу.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Продвинутая_лабораторная_работа_по_симплекс_методу"
       
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="500" 
        WindowStyle="None"
        WindowStartupLocation="CenterScreen" 
        Initialized="Window_Initialized">
    <Window.Background>
        <ImageBrush x:Name="BackGround" ImageSource="Content\Start.jpg"/>
    </Window.Background>
    <StackPanel Orientation="Horizontal" 
                VerticalAlignment="Bottom" >
        <TextBlock 
            Text="Установленная версия .NET framework: "
            
            x:Name="Consol"/>
        
    </StackPanel>
    
</Window>
